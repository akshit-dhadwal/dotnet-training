@{
}
@model List<Student>

    

  
     <br />
     <br />

    <table class="table">
      <thead>

        <tr>
          <th scope="col">Roll Number</th>
          <th scope="col">Name</th>
          <th scope="col">Age</th>
          <th scope="col">Edit</th>
          <th scope="col">Delete</th>
        </tr>
      </thead>
      <tbody>
           @foreach (var item in Model)
       {
           <tr>
               
                <td>@item.Roll</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@Html.ActionLink("Edit","Edit", new { id=@item.Roll})</td> 
                <td>@Html.ActionLink("Delete","Delete", new { id=@item.Roll})</td>
               
           </tr>
       }
  </tbody>
</table>

  <center>
    

     <button type="button" class="btn btn-primary"  onclick="location.href='@Url.Action("Insert", "Student")';return false;">Add new record</button>
     </center>

